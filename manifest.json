{
  "name": "GitHub Classic Theme",
  "version": "2020.6.23",
  "description": "Beautify the New GitHub UI",
  "manifest_version": 2,
  "icons": {
    "16": "icon/inactive32.png",
    "32": "icon/inactive32.png",
    "48": "icon/inactive64.png",
    "128": "icon/inactive128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs",
    "*://*.github.com/*"
  ],
  "page_action": {
    "default_icon": "icon/inactive32.png",
    "default_title": "GitHub Classic Theme",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "css": [
        "classic.css"
      ]
    }
  ]
}
